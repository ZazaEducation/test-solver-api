{
  "project": {
    "type": "fastapi",
    "python_version": "3.11+",
    "package_manager": "pip",
    "main_module": "src.ai_test_solver.main",
    "test_framework": "pytest"
  },
  "commands": {
    "dev": "uvicorn src.ai_test_solver.main:app --reload --host 0.0.0.0 --port 8000",
    "test": "pytest -v --cov=src --cov-report=term-missing",
    "lint": "ruff check src/ tests/ && black --check src/ tests/ && mypy src/",
    "format": "black src/ tests/ && ruff check --fix src/ tests/",
    "install": "pip install -e \".[dev]\"",
    "build": "python -m build",
    "clean": "rm -rf build/ dist/ *.egg-info/ .pytest_cache/ .mypy_cache/ .coverage htmlcov/"
  }
}